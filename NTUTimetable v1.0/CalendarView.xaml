            <Setter Property="Padding" Value="2,0,0,0" />
            <Setter Property="Foreground" Value="#404040" />
            <Setter Property="FontSize" Value="14" />

        </Style>

        <Style x:Key="DayTextBlock" TargetType="TextBlock">
